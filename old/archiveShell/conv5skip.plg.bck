#! /usr/bin/gnuplot

print GPVAL_VERSION
#set terminal dumb `echo $COLUMNS $LINES`
set terminal dumb COL LIN
print GPVAL_TERMOPTIONS
set multiplot layout 1,3
set title "E diff"
set logscale y
plot filename using 1:3 
set title "residue"
plot filename using 1:4
set autoscale y
set title "energy"
plot filename using 1:2 every ::5
unset multiplot

